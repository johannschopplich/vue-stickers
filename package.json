{
  "name": "vue-stickers",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@8.7.6",
  "description": "One component, multiple fancy sticker variants",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/vue-stickers#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/vue-stickers.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/vue-stickers/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "components.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "vue-tsc --noEmit",
    "release": "bumpp --commit --push --tag"
  },
  "peerDependencies": {
    "@vueuse/core": "*",
    "uncrypto": "^0.1.3",
    "vue": "^3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.8",
    "@types/node": "^20.6.5",
    "@unocss/eslint-config": "^0.56.2",
    "@unocss/reset": "^0.56.2",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vueuse/core": "^10.4.1",
    "bumpp": "^9.2.0",
    "eslint": "^8.50.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "uncrypto": "^0.1.3",
    "unocss": "^0.56.2",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.13"
  }
}
