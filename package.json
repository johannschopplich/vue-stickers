{
  "name": "vue-stickers",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@8.6.12",
  "description": "One component, multiple fancy sticker variants",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/vue-stickers#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/vue-stickers.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/vue-stickers/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "components.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "vue-tsc --noEmit",
    "release": "bumpp --commit --push --tag"
  },
  "peerDependencies": {
    "@vueuse/core": "^10",
    "uncrypto": "^0.1.3",
    "vue": "^3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@types/node": "^20.4.10",
    "@unocss/eslint-config": "^0.55.0",
    "@unocss/reset": "^0.55.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/core": "^10.3.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.47.0",
    "typescript": "^5.1.6",
    "unbuild": "^2.0.0-rc.0",
    "uncrypto": "^0.1.3",
    "unocss": "^0.55.0",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  }
}
